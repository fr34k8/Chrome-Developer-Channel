{
  "name": "chrome-developer-channel",
  "version": "1.0.0",
  "description": "Shows for web developers",
  "main": "service-worker.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm install -g polymer-cli && polymer build --entrypoint index.html --shell src/show-app/show-app.html",
    "build": "cd public && bower install",
    "test": "polymer test",
    "fix": "standard --parser babel-eslint",
    "html": "standard --plugin html '**/*.html'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sethbergman/Chrome-Developer-Channel.git"
  },
  "keywords": [
    "Polymer",
    "Progressive",
    "Web",
    "Apps",
    "PWA",
    "Firebase",
    "Node.js"
  ],
  "author": "Seth Bergman",
  "license": "MIT",
  "providedServices": {
  "hyperclick.provider": {
    "versions": {
      "0.0.0": "getProvider"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/sethbergman/Chrome-Developer-Channel/issues"
  },
  "homepage": "https://github.com/sethbergman/Chrome-Developer-Channel#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "bower": "^1.8.0",
    "firebase-tools": "^3.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "standard": "^10.0.2"
  }
}
