{
  "name": "chrome-developer-channel",
  "short_name": "show-app",
  "description": "Shows for web developers",
  "start_url": "/",
  "display": "standalone"
},
{
  "entrypoint": "index.html",
  "shell": "src/show-app/show-app.html",
  "fragments": [
    "src/show-app/show-list-page.html",
    "src/show-app/show-video-page.html"
  ],
  "sourceGlobs": [
   "src/**/*",
   "bower.json",
   "app.yaml"
  ],
  "includeDependencies": [
    "manifest.json",
    "polymer.json",
    "sw-import.js",
    "images/*",
    "bower_components/webcomponentsjs/webcomponents-lite.js"
  ]
}
